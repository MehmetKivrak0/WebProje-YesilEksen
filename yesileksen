--
-- TOC entry 320 (class 1255 OID 30266)
-- Name: uyelik_no_olustur(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.uyelik_no_olustur() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    IF NEW.oda_tipi = 'Ziraat' THEN
        NEW.uyelik_no = 'ZRT-' || TO_CHAR(CURRENT_DATE, 'YYYY') || '-' || LPAD(nextval('seq_ziraat_uyelik')::TEXT, 6, '0');
    ELSIF NEW.oda_tipi = 'Sanayi' THEN
        NEW.uyelik_no = 'SNY-' || TO_CHAR(CURRENT_DATE, 'YYYY') || '-' || LPAD(nextval('seq_sanayi_uyelik')::TEXT, 6, '0');
    END IF;
    RETURN NEW;
END;
$$;


ALTER FUNCTION public.uyelik_no_olustur() OWNER TO postgres;

