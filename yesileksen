--
-- TOC entry 5512 (class 2618 OID 30661)
-- Name: v_aktif_ciftlikler _RETURN; Type: RULE; Schema: public; Owner: postgres
--

CREATE OR REPLACE VIEW public.v_aktif_ciftlikler AS
 SELECT c.id,
    c.kullanici_id,
    c.ad,
    c.kayit_no,
    c.telefon,
    c.adres,
    c.sehir_id,
    c.enlem,
    c.boylam,
    c.alan,
    c.durum,
    c.kayit_tarihi,
    c.yillik_gelir,
    c.uretim_kapasitesi,
    c.organik,
    c.iyi_tarim,
    c.aciklama,
    c.website,
    c.olusturma,
    c.guncelleme,
    c.silinme,
    (((k.ad)::text || ' '::text) || (k.soyad)::text) AS sahip_adi,
    k.eposta,
    k.telefon AS kullanici_telefon,
    s.ad AS sehir_adi,
    count(DISTINCT u.id) AS urun_sayisi
   FROM (((public.ciftlikler c
     JOIN public.kullanicilar k ON ((c.kullanici_id = k.id)))
     LEFT JOIN public.sehirler s ON ((c.sehir_id = s.id)))
     LEFT JOIN public.urunler u ON ((c.id = u.ciftlik_id)))
  WHERE ((c.durum = 'aktif'::public.durum_tipi) AND (c.silinme IS NULL))
  GROUP BY c.id, k.id, s.id;


