--
-- TOC entry 269 (class 1259 OID 30658)
-- Name: v_aktif_ciftlikler; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.v_aktif_ciftlikler AS
SELECT
    NULL::uuid AS id,
    NULL::uuid AS kullanici_id,
    NULL::character varying(200) AS ad,
    NULL::character varying(100) AS kayit_no,
    NULL::character varying(20) AS telefon,
    NULL::text AS adres,
    NULL::smallint AS sehir_id,
    NULL::numeric(10,8) AS enlem,
    NULL::numeric(11,8) AS boylam,
    NULL::numeric(10,2) AS alan,
    NULL::public.durum_tipi AS durum,
    NULL::date AS kayit_tarihi,
    NULL::numeric(15,2) AS yillik_gelir,
    NULL::numeric(10,2) AS uretim_kapasitesi,
    NULL::boolean AS organik,
    NULL::boolean AS iyi_tarim,
    NULL::text AS aciklama,
    NULL::character varying(255) AS website,
    NULL::timestamp without time zone AS olusturma,
    NULL::timestamp without time zone AS guncelleme,
    NULL::timestamp without time zone AS silinme,
    NULL::text AS sahip_adi,
    NULL::character varying(255) AS eposta,
    NULL::character varying(20) AS kullanici_telefon,
    NULL::character varying(50) AS sehir_adi,
    NULL::bigint AS urun_sayisi;


ALTER VIEW public.v_aktif_ciftlikler OWNER TO postgres;

--
-- TOC entry 270 (class 1259 OID 30662)
-- Name: v_urunler_detay; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.v_urunler_detay AS
 SELECT u.id,
    u.ciftlik_id,
    u.kategori_id,
    u.ad,
    u.aciklama,
    u.birim_id,
    u.birim_fiyat,
    u.mevcut_miktar,
    u.min_siparis_miktari,
    u.durum,
    u.onerilir,
    u.goruntulenme,
    u.olusturma,
    u.guncelleme,
    u.silinme,
    c.ad AS uretici_adi,
    s.ad AS bolge,
    c.telefon AS uretici_telefon,
    c.organik,
    c.iyi_tarim,
    uk.ad AS kategori_adi,
    b.ad AS birim_adi,
    b.sembol AS birim_sembol
   FROM ((((public.urunler u
     JOIN public.ciftlikler c ON ((u.ciftlik_id = c.id)))
     LEFT JOIN public.sehirler s ON ((c.sehir_id = s.id)))
     JOIN public.urun_kategorileri uk ON ((u.kategori_id = uk.id)))
     JOIN public.birimler b ON ((u.birim_id = b.id)))
  WHERE (u.silinme IS NULL);


ALTER VIEW public.v_urunler_detay OWNER TO postgres;

