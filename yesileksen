CREATE FUNCTION public.bildirim_no_olustur() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    NEW.bildirim_no = 'BLD-' || TO_CHAR(CURRENT_DATE, 'YYYYMMDD') || '-' || LPAD(nextval('seq_bildirim_no')::TEXT, 6, '0');
    RETURN NEW;
END;
$$;


